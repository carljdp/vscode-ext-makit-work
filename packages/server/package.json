{
    "name": "server",
    "version": "0.0.1-dev",
    "publisher": "carljdp",
    "license": "Proprietary",
    "private": true,
    "author": {
        "name": "Carl J du Preez",
        "email": "carljdp@gmail.com",
        "url": "https://linktr.ee/carljdp"
    },
    "keywords": [
        "node",
        "typescript",
        "swc",
        "eslint"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "scripts": {
        "lint:report": "eslint --config eslint.config.mjs --cache --cache-location .cache",
        "lint:report-watch": "node ../../package/startx/devt/eslint-watch.mjs --config eslint.config.mjs --cache --cache-location .cache",
        "lint:fix-dry": "eslint --config eslint.config.mjs --cache --cache-location .cache --fix-dry-run",
        "lint:fix": "eslint --config eslint.config.mjs --cache --cache-location .cache --fix",
        "check-conf:eslint": "npx @eslint/config-inspector",
        "check-conf:tsconf": "npx tsc -p tsconfig.cjs.json --showConfig && npx tsc -p tsconfig.esm.json --showConfig",
        "typecheck": "tsc --project tsconfig.json --noEmit",
        "compile:pre": "node ../startx/devt/swc-configs-generate.js",
        "compile:cjs": "swc --cli-config-file .swc.cli.cjs.json src",
        "compile:esm": "swc --cli-config-file .swc.cli.esm.json src",
        "compile:post": "node ../startx/devt/copy-package-json.js",
        "serve:resources": "node ../server/dist/esm/cli.js --cli-server-args={\\\"path\\\":\\\"./dist/esm\\\",\\\"port\\\":\\\"3001\\\"}",
        "serve:explorer": "node ../server/dist/esm/cli.js --cli-server-args={\\\"filesAsHtml\\\":\\\"true\\\",\\\"port\\\":\\\"3002\\\"}",
        "start": "node dist/esm/index.js",
        "start-watch": "nodemon --watch dist --exec node dist/esm/index.js"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^0.4.8",
        "@eslint/js": "^9.3.0",
        "@swc-node/register": "^1.9.1",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.5.5",
        "@swc/helpers": "^0.5.11",
        "@swc/types": "^0.1.6",
        "@types/eslint": "^8.56.10",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^4.17.21",
        "@types/lodash": "^4.17.1",
        "@types/node": "^20.12.11",
        "@types/prismjs": "^1.26.4",
        "concurrently": "^8.2.2",
        "esbuild": "^0.21.3",
        "eslint": "^9.3.0",
        "globals": "^15.3.0",
        "lodash": "^4.17.21",
        "nodemon": "^3.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.10.0"
    },
    "dependencies": {
        "express": "^4.19.2",
        "prismjs": "^1.29.0",
        "sharp": "^0.33.4"
    },
    "engines": {
        "node": ">=21.7.3"
    }
}