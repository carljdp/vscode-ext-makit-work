    "license": "Proprietary",
    "private": true,
    "type": "module",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "scripts": {
        "reinstall": "npx pnpm install --loglevel error",
        "check-eslint": "npx @eslint/config-inspector",
        "check-tsconf": "npx tsc -p tsconfig.cjs.json --showConfig && npx tsc -p tsconfig.esm.json --showConfig",
        "lint-once": "eslint --config eslint.config.mjs --cache --cache-location .cache",
        "lint-watch": "node ../../packages/startx/devt/eslint-watch.mjs --config eslint.config.mjs --cache --cache-location .cache",
        "lint-fix:dry": "eslint --config eslint.config.mjs --cache --cache-location .cache --fix-dry-run",
        "lint-fix:do": "eslint --config eslint.config.mjs --cache --cache-location .cache --fix",
        "lint-types:cjs": "tsc --project tsconfig.cjs.json --noEmit",
        "lint-types:esm": "tsc --project tsconfig.esm.json --noEmit",
        "build-run:pre": "node ../../packages/startx/devt/swc-configs-generate.js",
        "build-swc:cjs": "swc --cli-config-file .swc.cli.cjs.json src",
        "build-swc:esm": "swc --cli-config-file .swc.cli.esm.json src",
        "build-run:post": "node ../../packages/startx/devt/copy-package-json.mjs",
        "build-all": "npm run build-run:pre && npm run build-swc:cjs && npm run build-swc:esm && npm run build-run:post",
        "serve-dist:esm:once": "node ../server/dist/esm/cli.js --cli-server-args={\\\"path\\\":\\\"./dist/esm\\\",\\\"port\\\":\\\"5001\\\"}"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^0.4.8",
        "@eslint/js": "^9.3.0",
        "@swc-node/register": "^1.9.1",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.5.7",
        "@types/chai": "^4.3.16",
        "@swc/helpers": "^0.5.11",
        "@swc/types": "^0.1.6",
        "@types/eslint": "^8.56.10",
        "@types/eslint__js": "^8.42.3",
        "@types/lodash": "^4.17.4",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.12.11",
        "esbuild": "^0.21.3",
        "eslint": "^9.3.0",
        "globals": "^15.3.0",
        "lodash": "^4.17.21",
        "chai": "^5.1.1",
        "mocha": "^10.4.0",
        "nyc": "^15.1.0",
        "startx": "workspace:../../packages/startx/dist",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.10.0",
        "@types/three": "^0.164.0"
    },
    "dependencies": {
        "ejs": "^3.1.10",
        "three": "^0.164.1"
    },
    "engines": {
        "node": "^21.7.3"
    },
    "workspaces": [
    ],
    "$schema": "https://json.schemastore.org/package.json"
}