{
    "name": "preloader",
    "version": "0.0.1",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "scripts": {
        "clean": "rimraf dist",
        "compile:pre": "node ./scripts/compile-pre.mjs",
        "compile:cjs": "swc --cli-config-file .swc.cli.cjs.json src",
        "compile:esm": "swc --cli-config-file .swc.cli.esm.json src",
        "compile:suf": "node ./scripts/compile-suf.mjs preloader scripts dist",
        "compile": "pnpm run compile:pre && npm-run-all --parallel compile:cjs compile:esm && pnpm run compile:suf"
    },
    "devDependencies": {
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.4.17",
        "@swc/helpers": "^0.5.11",
        "@swc/types": "^0.1.6",
        "@types/node": "^20.12.8",
        "dotenv": "^16.4.5",
        "lodash": "^4.17.21",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}