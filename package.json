{
  "name": "makit-work",
  "displayName": "Makit Work",
  "description": "An experimental extension to mimic built-in Profiles",
"author": {
    "name": "Carl J du Preez",
    "email": "carljdp@gmail.com",
    "url": "https://linktr.ee/carljdp"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "makit-work.enableExtension",
        "title": "makit-work.enableExtension"
      },
      {
        "command": "makit-work.disableExtension",
        "title": "makit-work.disableExtension"
      },
      {
        "command": "makit-work.helloWorld",
        "title": "makit-work.helloWorld"
      }
    ]
  },
  "viewsContainers": {
    "activitybar": [
      {
        "id": "makit-work.extensionManager",
        "title": "Extension Manager",
        "icon": "media/extension-icon.svg"
      }
    ]
  },
  "views": {
    "extensionManager": [
      {
        "id": "makit-work.extensionView",
        "name": "Manage Extensions"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
