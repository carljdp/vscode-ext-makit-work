{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "jsx": false,
            "dynamicImport": false,
            "privateMethod": false,
            "functionBind": false,
            "exportDefaultFrom": false,
            "exportNamespaceFrom": false,
            "decorators": true,
            "decoratorsBeforeExport": false,
            "topLevelAwait": false,
            "importMeta": false,
            "importAssertions": false
        },
        "transform": {
            "constModules": {},
            "optimizer": {},
            "decoratorVersion": "2021-12"
        },
        "target": "es2022"
    },
    "module": {
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false,
        "type": "es6",
        "importInterop": "swc",
        "exportInteropAnnotation": false
    },
    "sourceMaps": true,
    "inputSourceMap": true,
    "$schema": "https://swc.rs/schema.json"
}