{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Current TS File",
            "runtimeArgs": [
                "--import=./preloader.js"
            ],
            "args": [
                "${relativeFile}"
            ],
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}