{
    "name": "bootstrap",
    "version": "0.0.1-dev",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "scripts": {
        "compile:pre": "node .devtools/swc-configs-generate.js",
        "compile:cjs": "swc --cli-config-file .swc.cli.cjs.json src",
        "compile:esm": "swc --cli-config-file .swc.cli.esm.json src",
        "compile:post": "node .devtools/copy-package-json.js",
        "compile": "npx pnpm run compile:pre && npx pnpm run compile:cjs & npx pnpm run compile:esm"
    },
    "devDependencies": {
        "@swc-node/register": "^1.9.1",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.5.5",
        "@swc/helpers": "^0.5.11",
        "@swc/types": "^0.1.6",
        "@types/lodash": "^4.17.1",
        "@types/node": "^20.12.11",
        "concurrently": "^8.2.2",
        "dotenv": "^16.4.5",
        "eslint": "^9.2.0",
        "globals": "^15.2.0",
        "json5": "^2.2.3",
        "jsonc-parser": "^3.2.1",
        "lodash": "^4.17.21",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "type-fest": "^4.18.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.8.0"
    },
    "engines": {
        "node": ">=21.7.3"
    }
}