{
    "name": "preloader",
    "version": "0.0.1",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "scripts": {
        "clean": "rimraf dist",
        "compile:pre": "node ./.dev/swc-configs-generate.mjs",
        "compile:cjs": "swc --cli-config-file .swc.cli.cjs.json src",
        "compile:esm": "swc --cli-config-file .swc.cli.esm.json src",
        "compile:suf": "node ./.dev/compile-suf.mjs preloader .dev dist",
        "compile": "pnpm run compile:pre && npm-run-all --parallel compile:cjs compile:esm && pnpm run compile:suf"
    },
    "devDependencies": {
        "@swc-node/register": "^1.9.1",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.5.5",
        "@swc/helpers": "^0.5.11",
        "@swc/types": "^0.1.6",
        "@types/lodash": "^4.17.1",
        "@types/node": "^20.12.11",
        "dotenv": "^16.4.5",
        "lodash": "^4.17.21",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.5",
        "typescript": "^5.4.5",
        "globals": "^15.2.0",
        "eslint": "^9.2.0",
        "typescript-eslint": "^7.8.0"
    },
    "engines": {
        "node": ">=21.7.3"
    }
}